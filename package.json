{
  "name": "metal-cloud-ui",
  "version": "0.0.9",
  "description": "A react component library for metal cloud.",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:js ",
    "build:js": "rollup -c --watch",
    "build:types": "tsc --emitDeclarationOnly --outDir dist --watch",
    "dev": "concurrently \"rollup -c -w\" \"npm run storybook\" ",
    "storybook": "storybook dev -p 6006",
    "storybook:css": "npx tailwindcss -w -i ./src/index.css -o ./src/index.css",
    "build-storybook": "concurrently \"npm run build-storybook:css\" \"storybook build\"",
    "build-storybook:css": "tailwindcss -m -i ./src/index.css -o ./src/index.css"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NowPurchase/Aui.git"
  },
  "author": "Vikash",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NowPurchase/Aui/issues"
  },
  "homepage": "https://github.com/NowPurchase/Aui#readme",
  "devDependencies": {
    "@babel/core": "^7.24.8",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^1.6.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.2.2",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/string-hash": "^1.1.3",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-preset-react-app": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.6.0",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "storybook": "^8.1.11",
    "string-hash": "^1.1.3",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
